body {
    font-family: 'Helvetica Neue',Helvetica,sans-serif;
    font-size: 17px;
    line-height: 21px;
    color: #000;
    background-color: #efeff4;
    -webkit-overflow-scrolling: touch;
}
*{margin: 0;padding: 0;}
.phone {
    margin: 0 auto;
    position: relative;
    background: #111;
    border-radius: 55px;
    box-shadow: 0px 0px 0px 2px #aaa;
    width: 320px;
    height: 568px;
    padding: 105px 25px;
    -webkit-box-sizing: content-box;
    box-sizing: content-box;
}
.phone:before {
    content: '';
    width: 60px;
    height: 10px;
    border-radius: 10px;
    position: absolute;
    left: 50%;
    margin-left: -30px;
    background: #333;
    top: 50px;
}
.phone .statusbar {
    position: absolute;
    width: 320px;
    height: 20px;
    background: url('data:image/png;base64,');
    left: 50%;
    margin-left: -160px;
    top: 100px;
    -webkit-background-size: 100% auto;
    background-size: 100% auto;
}
.phone .content {
    position: relative;
    height: 100%;
    width: 100%;
    margin-top: 20px;
}
.iphone-bar {
    position: absolute;
    z-index: 10;
    right: 0;
    left: 0;
    top: 0;
    height: 44px;
    padding:0 10px;
    border-bottom: 1px solid #f4f4f4;
    background-color: #fff;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}
.iphone-bar .title {
    right: 40px;
    left: 40px;
    display: inline-block;
    overflow: hidden;
    width: auto;
    margin: 0;
    text-overflow: ellipsis;
}
.title {
    font-size: 17px;
    font-weight: 500;
    line-height: 44px;
    position: absolute;
    display: block;
    width: 100%;
    margin: 0 -10px;
    padding: 0;
    text-align: center;
    white-space: nowrap;
    color: #000;
}
.iphone-iframe-wrapper {
    position: absolute;
    overflow-y: scroll;
    right: 0;
    left: 0;
    top: 45px; bottom: 0;
    -webkit-overflow-scrolling: touch;
    background-color: #f5f5f5;
}
.question-item{
    background-color: #fff;
    margin-bottom: 10px;
    padding-bottom: 10px;
    font-size:14px;
}
.question-item .question-title{padding: 5px 10px;line-height: 1.5;color: #000;}
.question-item ul li{list-style: none;position: relative;padding-left: 10px;line-height: 3;color: #666;}
.question-item ul li:after{position: absolute;content: '';height: 1px;overflow: hidden;left: 10px;right:10px;
    top: 40px; background-color: #f4f4f4;}
.question-item textarea{height: 70px;margin: 10px auto;width: 93%;display: block; border: 0; padding: 5px;color: #666;resize: none;background-color: #f1f6ff;}
.question-item input[type=range] {display: block;width: 93%;margin: 10px auto;  }
.question-item  .upload{height: 44px;margin: 10px auto;width: 93%;display: block;padding: 10px 5px;background-color: #f1f6ff;}
.question-item  .upload .btn{display: inline-block;text-indent: -9999;
    width: 44px;
    height: 44px;border: 0;outline: none;
    background: url('data:image/png;base64,');
    background-size: 100%;
}
.range-text{padding: 0px 10px;overflow: hidden;}
.range-text span{color: #1c8ed8;}